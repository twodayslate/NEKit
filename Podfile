# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

def common_pods
pod 'CocoaAsyncSocket'
pod 'CocoaLumberjack/Swift'
pod 'MMDB-Swift'
pod 'Sodium'
pod 'Yaml'
pod 'Resolver', :git => 'https://github.com/twodayslate/Resolver.git'
end

target 'NEKit-iOS' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NEKit-iOS

  common_pods

end

target 'NEKit-macOS' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NEKit-macOS

  common_pods

end

target 'NEKitDemo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NEKitDemo

  common_pods

end
